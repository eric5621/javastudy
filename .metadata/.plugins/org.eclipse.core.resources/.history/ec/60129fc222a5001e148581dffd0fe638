package 산술연산자;

import java.util.Scanner;

public class Ex02산술연산자예제 {

	public static void main(String[] args) {
 
		// 입력도구 가져오기
		Scanner sc = new Scanner(System.in) ;
		// 입력 받기
		String name = sc.next();
		
		System.out.println("입력받은 글자는 >> " + name);
		
		System.out.print("숫자를 입력하세요 >> ");
		// 정수형 숫자를 입력받기
		int num1 = sc.nextInt();
		
		
		
		
		
		
		
	}

}
