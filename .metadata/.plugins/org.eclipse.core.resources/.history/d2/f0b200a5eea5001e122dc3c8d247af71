	public static void main(String[] args) {

		Scanner sc = new Scanner(System.in);
		
		int num = sc.nextInt();
		int num1 = 15;
		System.out.println("정수를 입력하세요 : ");		
		if (num%3==0) {
			System.out.println("3과 5의 배수입니다");
		}
		
		
		
		
		
		
		
		
		
	}

}
